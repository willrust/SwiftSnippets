DELEGATE_PROTO_SNIPPET=68013514-C5F9-46D4-A7BC-284D7F028720.codesnippet
DELEGATE_PROPERTY_SNIPPET=B9294F60-93C2-4039-98E6-E501419A6ED1.codesnippet
UNWRAPPING_IF_SNIPPET=4B291E29-B387-4B0D-9510-8FF06BFFB6E8

DESTDIR=~/Library/Developer/Xcode/UserData/CodeSnippets

install:
	mkdir -p $(DESTDIR)
	cp CodeSnippets/$(DELEGATE_PROTO_SNIPPET) $(DESTDIR)
	cp CodeSnippets/$(DELEGATE_PROPERTY_SNIPPET) $(DESTDIR)
	cp CodeSnippets/$(UNWRAPPING_IF_SNIPPET) $(DESTDIR)

uninstall:
	rm $(DESTDIR)/$(DELEGATE_PROTO_SNIPPET)
	rm $(DESTDIR)/$(DELEGATE_PROPERTY_SNIPPET)
	rm $(DESTDIR)/$(UNWRAPPING_IF_SNIPPET)